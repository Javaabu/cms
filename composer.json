{
    "name": "javaabu/cms",
    "description": "Content Management System integration for Laravel Projects",
    "homepage": "https://github.com/Javaabu/cms",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "cms"
    ],
    "authors": [
        {
            "name": "FlameXode",
            "email": "wovencoast14782@techie.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "javaabu/helpers": "^1.61",
        "javaabu/translatable": "^1.1",
        "javaabu/menu-builder": "^1.6",
        "kalnoy/nestedset": "^6.0",
        "javaabu/auth": "^1.16",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "lorisleiva/laravel-actions": "^2.9"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^9.5 || ^10.5 || ^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Cms\\": "src/",
            "Javaabu\\Cms\\Database\\Factories\\": "database/factories/",
            "Javaabu\\Cms\\Database\\Seeders\\": "database/seeders/"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Cms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Cms\\CmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
